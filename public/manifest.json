{
  "manifest_version": 3,
  "name": "TheColor",
  "description": "Direct Link Extractor",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["scripting"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "icons": {
    "32": "icon.png",
    "48": "icon.png",
    "72": "icon.png",
    "128": "icon.png",
    "256": "icon.png"
  }
}
